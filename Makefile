byebye:(

endef
export RAT
export GOODBYE
SRCS =  main.c
OBJECTS = ${SRCS:.c=.o}
OBJDIR = obj
OBJECTFILES = $(addprefix $(OBJDIR)/, $(OBJECTS))
MLXFLAGS = -Lmlx -lmlx -lX11 -lXext -lm 
CC = cc
CFLAGS = -Wall -Wextra -Werror
$(OBJDIR)/%.o: %.c main.h
	@mkdir -p $(OBJDIR)
	@$(CC) $(CFLAGS) -c $< -o $@

$(NAME): $(OBJECTFILES)
	@$(CC) $(CFLAGS) $(MLXFLAGS) $(OBJECTFILES) -o $(NAME)
	@printf "$$RAT"

all: $(NAME)

clean:
	@rm -rf $(OBJDIR)
	@printf "$$GOODBYE"

fclean: clean
	@rm -f $(NAME)

re: fclean all

.PHONY: all clean fclean re
